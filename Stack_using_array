#Creating a stack
def createstack():
    stack = []
    return stack

#creating an empty stack
def checkempty(stack):
    return len(stack) == 0
    
#adding item to a stack
def push(stack,item):
    stack.append(item)
    print("pushed item :" + item)
    
#removing an element from the satck
def pop(stack):
    if (checkempty(stack)):
        return "stack is empty"
    return stack.pop()
stack = createstack()
push(stack,str(1))
push(stack,str(2))
push(stack,str(3))
push(stack,str(4))
print ("poped item : "+pop(stack))
print("stack after poping an element" +str(stack))
 
